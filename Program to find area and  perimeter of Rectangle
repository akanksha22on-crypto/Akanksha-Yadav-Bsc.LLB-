#include <stdio.h>
void main()
{
    int l,b;
    int area,perimeter;
    printf("Enter the length and breadth of the rectangle:");
    scanf("%d",&l);
    scanf("%d",&b);
    area=l*b;
    printf("Area of rectangle :%d\n",area);
    perimeter=2*(l+b);
    printf("Perimeter of rectangle :%d\n",perimeter);

}
