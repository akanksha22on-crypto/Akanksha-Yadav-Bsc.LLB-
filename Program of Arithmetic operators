#include <stdio.h>
void main()
{
    int a,b,result;
    float div;
    printf("Enter two values:");
    scanf("%d",&a);
    scanf("%d",&b);
    result=a+b;
    printf("Sum of %d & %d= %d\n",a,b,result);
    printf("Difference of %d & %d= %d\n",a,b,a-b);
    printf("Factor of %d & %d = %d\n",a,b,a*b);
    div=a/b;
    printf("Division of %d & %d = %f/n",a,b,div);
}
